model Developer {
  id                    String    @id @default(uuid())
  baseUserId            String    @unique
  baseUser              BaseUser  @relation(fields: [baseUserId], references: [id])
  createdUsersCount     Int       @default(0)
  balance              Float     @default(10000000000) @map("Balance")
  freeChips            Float     @default(10000000000) @map("freeChips")
  
  @@map("developers")
}